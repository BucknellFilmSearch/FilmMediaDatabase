{
  "name": "filmtvse_server",
  "version": "0.1.0",
  "description": "A skeleton for node projects with babel, eslint, and gulp",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src",
    "start": "nodemon --exec ./node_modules/babel-cli/bin/babel-node.js src/index.js",
    "clean": "rm -rf ./build",
    "build": "rm -rf ./build && gulp",
    "detach-git": "echo 'To add a new repository use: \"git remote add origin [remote address]\"' && git remote rm origin"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "lodash": "^4.17.5",
    "pg": "^7.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "eslint": "^3.19.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-delete-lines": "^0.0.7",
    "gulp-json-editor": "^2.2.1",
    "nodemon": "^1.11.0"
  }
}
