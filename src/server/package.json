{
  "name": "filmtvse_server",
  "version": "0.1.0",
  "description": "A skeleton for node projects with babel, eslint, and gulp",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src",
    "dev-static": "watchify ./src/static/js/app.jsx -o ./src/static/js/bundle.js -t [ envify ] -t [ babelify --presets [ es2015 react ] --plugins [transform-object-rest-spread transform-decorators-legacy] ] -v",
    "dev-server": "nodemon src/index.js",
    "clean": "gulp clean",
    "build": "gulp build"
  },
  "nodemonConfig": {
    "ignore": [
      "src/static/**/*"
    ],
    "execMap": {
      "js": "./node_modules/babel-cli/bin/babel-node.js"
    }
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.4",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "material-ui": "^0.16.7",
    "material-ui-fullscreen-dialog": "^0.2.0",
    "morgan": "^1.9.0",
    "pg": "^7.4.1",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-dropzone": "^3.11.0",
    "react-fabricjs": "^0.1.6",
    "react-image-gallery": "^0.7.12",
    "react-lazyload": "^2.2.4",
    "react-onscroll": "^1.0.1",
    "react-redux": "^5.0.2",
    "react-render-html": "^0.6.0",
    "react-router": "^3.0.2",
    "react-slick": "^0.18.0",
    "react-tap-event-plugin": "^2.0.1",
    "react-tooltip": "^3.2.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^2.5.4",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "envify": "^4.0.0",
    "eslint": "^3.19.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-delete-lines": "^0.0.7",
    "gulp-json-editor": "^2.2.1",
    "nodemon": "^1.11.0",
    "react-addons-perf": "^15.4.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.9.0"
  }
}
